<Application x:Class="MyMortgage.Wpf.Runner.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:runner="clr-namespace:MyMortgage.Wpf.Runner"
             StartupUri="Shell.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/MyMortgage.Wpf.Core;component/Themes/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <runner:ViewBootstrap x:Key="ViewBootstrap" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
